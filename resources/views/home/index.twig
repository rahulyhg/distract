{% extends 'app.twig' %}

{% block content %}
    <div v-if="loading" class="loading">One moment</div>
    <div v-else class="item" v-for="item in items">
        <h3 class="item__title">
            <a :href="item.link" target="_blank">${ item.title }</a>
        </h3>
        <div class="item__service">${ item.service }</div>
    </div>
{% endblock %}
